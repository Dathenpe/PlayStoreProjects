<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Heal" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/orange_accent</item>
        <item name="colorPrimaryVariant">@color/orange_accent</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/orange_accent</item>
        <item name="android:statusBarColor">@color/transparent</item>

    </style>

    <style name="Theme.Heal.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Heal.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Heal.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>